# MidScene Server 环境变量配置
# 复制此文件为 .env 并填入实际值

# 服务器配置
PORT=3000
NODE_ENV=development

# 日志配置
CLS_ENDPOINT=""
CLS_TOPIC_ID=""
LOG_LEVEL=info

# VL-qwen 相关配置
OPENAI_API_KEY=""
OPENAI_BASE_URL=""
MIDSCENE_MODEL_NAME=""
MIDSCENE_USE_QWEN3_VL=1

# midscene 核心配置
MIDSCENE_CACHE=1
MIDSCENE_REPLANNING_CYCLE_LIMIT=3 # 默认值为 10，可根据需求调整

# mastra 配置
MASTRA_TELEMETRY_DISABLED=1

# midscene LangSmith 调试配置
MIDSCENE_LANGSMITH_DEBUG=1

# LangSmith 链路追踪配置
LANGSMITH_TRACING_V2=true
LANGSMITH_ENDPOINT="https://api.smith.langchain.com"
LANGSMITH_API_KEY=""
LANGSMITH_PROJECT="midscene-server"

# OSS 对象存储配置（腾讯云 COS）
COS_SECRET_ID=""
COS_SECRET_KEY=""
COS_BUCKET=""
COS_REGION=""
COS_URL=""