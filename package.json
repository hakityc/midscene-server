{
  "name": "midscene-server",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "tsx src/index.ts",
    "test:logger": "tsx src/test/logger-test.ts",
    "mcp:server:stdio": "tsx src/mastra/mcp/server/start-server.ts stdio",
