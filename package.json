    "@mastra/core": "0.15.2",
    "@mastra/loggers": "0.10.9",
    "@mastra/mcp": "0.11.2",
    "dotenv": "^17.2.1",
    "hono": "^4.9.5",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.2",
    "@midscene/web": "^0.27.6",
    "@types/node": "^20.19.11",
    "mastra": "0.11.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
